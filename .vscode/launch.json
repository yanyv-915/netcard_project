{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "调试 Netcard Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/main.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "D:/MinGW/bin/gdb.exe", // 按实际路径修改
      "setupCommands": [
        {
          "description": "启用 pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build netcard project"
    }
  ]
}
